<!DOCTYPE html>
<html lang="en">
<head>
  {% include '@Example/partials/common/htmlhead.html.twig' %}
</head>

<body>
  {% include '@Example/partials/common/checkout-header.html.twig' %}
  <div class="checkout-shipping">
    <div class="container">
      {% include '@Example/partials/common/messages.html.twig' %}
      <div class="row">
        <div class="col-md-6">
          {% include '@Example/partials/checkout/secure-checkout-title.html.twig' with {'titleKey':"checkout:shippingPage.title"} %}
        </div>
        <div class="col-md-6">
          {% include '@Example/partials/checkout/step-widget.html.twig' %}
        </div>
      </div>
      <div class="row">
        <div class="col-sm-12">
          <div class="checkout">
            <div class="row">
              <div class="col-sm-7">
                {% include '@Example/partials/checkout/shipping-form.html.twig' %}
              </div>
              <div class="col-sm-5">
                {% include '@Example/partials/checkout/order-summary-sidebar.html.twig' %}
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  {% include '@Example/partials/common/checkout-footer.html.twig' %}
  {% include '@Example/partials/common/htmlscripts.html.twig' %}
</body>

</html>
