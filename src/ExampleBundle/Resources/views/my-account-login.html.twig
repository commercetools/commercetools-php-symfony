<!DOCTYPE html>
<html lang="en">

<head>
  {% include '@Example/partials/common/htmlhead.html.twig' %}
</head>

<body>
  <div class="darkbg hidden"></div>
  {% include '@Example/partials/common/header.html.twig' %}
  <div class="container">
    {% include '@Example/partials/common/messages.html.twig' %}

    <div class="my-account-login-page-title">
      <span class="icon-user">{{ 'pageTitle'|trans([], 'my-account-login') }}</span>
    </div>

    <div class="login-signup-box-wrapper">
      <div class="row">
        <div class="col-sm-6">
          {% include '@Example/partials/myaccount/login-box.html.twig' with {'form':content.logInForm, 'settings':content.logInFormSettings} %}
        </div>
        <div class="col-sm-6">
          <div class="signup-box-mobile">
            {% include '@Example/partials/myaccount/signup-box.html.twig' with {'form':content.signUpForm, 'settings':content.signUpFormSettings} %}
          </div>
        </div>
      </div>
    </div>

  </div>
  {% include '@Example/partials/common/footer.html.twig' %}
  {% include '@Example/partials/common/htmlscripts.html.twig' %}
</body>
</html>
