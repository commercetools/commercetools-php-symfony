<!DOCTYPE html>
<html lang="en">

<head>
  {% include '@Example/partials/common/htmlhead.html.twig' %}
</head>

<body>
  <div class="darkbg hidden"></div>
  {% include '@Example/partials/common/header.html.twig' %}
    <div class="container">
      {% include '@Example/partials/common/messages.html.twig' %}

    <div class="my-account-login-page-title">
      <span class="icon-user">{{ 'contactUs.title'|trans([], 'catalog') }}</span>
    </div>

    <div class="contact-form-content-wrapper">
      <div class="contact-form-title-row">
        <span class="contact-form-title">{{ 'contactForm.pleaseFill'|trans([], 'main') }}</span>
        <span class="hidden-xs contact-form-required">{{ 'form.required'|trans([], 'main') }}*</span>
      </div>
      <div class="contact-form-description">
        <div>{{ 'contactForm.customerSupport'|trans([], 'main') }}</div>
        <div>{{ 'contactForm.callUsOn'|trans([], 'main') }}</div>
        <div class="contact-form-description-respond">{{ 'contactForm.responseTime'|trans([], 'main') }}</div>
      </div>
      <div>
        {% include '@Example/partials/form/contact-form-form.html.twig' with {'form':form, 'settings':content.contactFormSettings} %}
      </div>
    </div>

  </div>
  {% include '@Example/partials/common/footer.html.twig' %}
  {% include '@Example/partials/common/htmlscripts.html.twig' %}
</body>
</html>
