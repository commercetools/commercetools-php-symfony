<!DOCTYPE html>
<html lang="en">

<head>
  {% include '@Example/partials/common/htmlhead.html.twig' %}
</head>

<body>
  <div class="darkbg hidden"></div>
  {% include '@Example/partials/common/header.html.twig' %}

    <div class="container">
      {% include '@Example/partials/common/messages.html.twig' %}
    <div class="faq-title">{{ 'faq.title'|trans([], 'main') }}</div>
    <div class="faq-wrapper">
      <div class="faq-section-title">{{ 'faq.delivery'|trans([], 'main') }}</div>
      {% for deliveryFaqs_i in content.deliveryFaqs %}
        {% include '@Example/partials/faq/faq-accordion.html.twig' with {'key':"delivery"} %}
      {% endfor %}
      <div class="faq-section-title">{{ 'faq.returns'|trans([], 'main') }}</div>
      {% for returnsFaqs_i in content.returnsFaqs %}
        {% include '@Example/partials/faq/faq-accordion.html.twig' with {'key':"returns"} %}
      {% endfor %}
    </div>
  </div>

  {% include '@Example/partials/common/footer.html.twig' %}
  {% include '@Example/partials/common/htmlscripts.html.twig' %}
</body>

</html>
