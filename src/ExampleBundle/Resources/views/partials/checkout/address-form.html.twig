{{ form_start(form, {'action' : path('_ctp_example_checkout_address'), 'method': 'POST'}) }}
  {{ form_row(form._token) }}
  <div class="shipping-info">
    <span class="text-uppercase shipping-info-title">{{ 'shippingInformation'|trans([], 'checkout') }}</span>
    <span class="pull-right required-fields">{{ 'form.required'|trans([], 'main') }}*</span>
  </div>
    {% set formObject = form %}
    {% include '@Example/partials/form/global-errors.html.twig' %}
    {% set formObject = form.shippingAddress %}
    <div class="row">
        <div class="col-sm-4">
            {% include '@Example/partials/form/choose-title.html.twig' with {'containerClass':"title", 'formFieldName':'title'} %}
        </div>
    </div>
    <div class="row">
        {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':6, 'required': true, 'formFieldName':'firstName'} %}
        {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':6, 'required':true, 'formFieldName':'lastName'} %}
        {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':12, 'required':true, 'formFieldName':'streetName'} %}
        {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':12, 'required':false, 'formFieldName':'additionalStreetInfo'} %}
        {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':8, 'required':true, 'formFieldName':'city'} %}
        {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':4, 'required':true, 'formFieldName':'postalCode'} %}
        <div class="col-sm-6">
            {% include '@Example/partials/form/choose-country.html.twig' with {'containerClass':"country", 'formFieldName':'country'} %}
        </div>
        {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':6, 'required':false, 'formFieldName':'region'} %}
    </div>
    <hr>
    <div class="row">
        {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':6, 'required':false, 'formFieldName':'phone'} %}
        {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':6, 'required':true, 'formFieldName':'email'} %}
    </div>
  <div class="row different-billing-row">
    <div class="col-sm-12">
      <div class="different-for-billing input-block">
        <label for="different-billing-checkbox">
          <span>
              {{ form_widget(form.differentAddresses, {'id': 'different-billing-checkbox'}) }}
          </span>
          <span>{{ 'shippingPage.differentBillingAddressCheckboxLabel'|trans([], 'checkout') }}</span>
        </label>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-sm-12">
      <div id="different-billing-address">
        <div class="row">
            {% set formObject = form.billingAddress %}

            <div class="row">
                <div class="col-sm-4">
                    {% include '@Example/partials/form/choose-title.html.twig' with {'containerClass':"title", 'formFieldName':'title'} %}
                </div>
            </div>
            <div class="row">
                {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':6, 'required': true, 'formFieldName':'firstName'} %}
                {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':6, 'required':true, 'formFieldName':'lastName'} %}
                {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':12, 'required':true, 'formFieldName':'streetName'} %}
                {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':12, 'required':false, 'formFieldName':'additionalStreetInfo'} %}
                {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':8, 'required':true, 'formFieldName':'city'} %}
                {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':4, 'required':true, 'formFieldName':'postalCode'} %}
                <div class="col-sm-6">
                    {% include '@Example/partials/form/choose-country.html.twig' with {'containerClass':"country", 'formFieldName':'country'} %}
                </div>
                {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':6, 'required':false, 'formFieldName':'region'} %}
            </div>
            <hr>
            <div class="row">
                {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':6, 'required':false, 'formFieldName':'phone'} %}
                {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':6, 'required':true, 'formFieldName':'email'} %}
            </div>
        </div>
      </div>
    </div>
  </div>
  {% include '@Example/partials/checkout/continue-checkout-button.html.twig' %}
{{ form_end(form, {'render_rest':false}) }}
