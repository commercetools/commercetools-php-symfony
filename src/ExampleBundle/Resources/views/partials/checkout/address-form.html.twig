<form method="post" action="{{ root.meta._links.checkoutAddressesSubmit.href }}">
  <input type="hidden" name="csrfToken" value="{{ csrf_token('csrfToken') }}"/>
  <div class="shipping-info">
    <span class="text-uppercase shipping-info-title">{{ 'shippingInformation'|trans([], 'checkout') }}</span>
    <span class="pull-right required-fields">{{ 'form.required'|trans([], 'main') }}*</span>
  </div>
  {% include '@Example/partials/form/global-errors.html.twig' with {'errors':addressForm.errors} %}
    <div class="row">
        <div class="col-sm-4">
            {% include '@Example/partials/form/choose-title.html.twig' with {'containerClass':"title", 'selectId':"shipping-title-select", 'selectName':"titleShipping", 'options':settings.titleShipping} %}
        </div>
    </div>
    <div class="row">
        {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':6, 'name':"firstNameShipping", 'labelKey':"form.firstName", 'required':true, 'value':addressForm.firstNameShipping, 'settings':settings.firstNameShipping} %}
        {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':6, 'name':"lastNameShipping", 'labelKey':"form.lastName", 'required':true, 'value':addressForm.lastNameShipping, 'settings':settings.lastNameShipping} %}
        {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':12, 'name':"streetNameShipping", 'labelKey':"form.addressOne", 'required':true, 'value':addressForm.streetNameShipping, 'settings':settings.streetNameShipping} %}
        {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':12, 'name':"additionalStreetInfoShipping", 'labelKey':"form.addressTwo", 'required':false, 'value':addressForm.additionalStreetInfoShipping, 'settings':settings.additionalStreetInfoShipping} %}
        {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':8, 'name':"cityShipping", 'labelKey':"form.city", 'required':true, 'value':addressForm.cityShipping, 'settings':settings.cityShipping} %}
        {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':4, 'name':"postalCodeShipping", 'labelKey':"form.postCode", 'required':true, 'value':addressForm.postalCodeShipping, 'settings':settings.postalCodeShipping} %}
        <div class="col-sm-6">
            {% include '@Example/partials/form/choose-country.html.twig' with {'containerClass':"country", 'selectId':"shipping-country-select", 'selectName':"countryShipping", 'options':settings.countriesShipping} %}
        </div>
        {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':6, 'name':"regionShipping", 'labelKey':"form.region", 'required':false, 'value':addressForm.regionShipping, 'settings':settings.regionShipping} %}
    </div>
    <hr>
    <div class="row">
        {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':6, 'name':"phoneShipping", 'labelKey':"form.phone", 'required':false, 'value':addressForm.phoneShipping, 'settings':settings.phoneShipping} %}
        {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':6, 'name':"emailShipping", 'labelKey':"form.email", 'required':true, 'value':addressForm.emailShipping, 'settings':settings.emailShipping} %}
    </div>
  <div class="row different-billing-row">
    <div class="col-sm-12">
      <div class="different-for-billing input-block">
        <label for="different-billing-checkbox">
          <span>
            <input id="different-billing-checkbox" type="checkbox" value="true" name="billingAddressDifferentToBillingAddress"{% if addressForm.billingAddressDifferentToBillingAddress %} checked{% endif %}>
          </span>
          <span>{{ 'shippingPage.differentBillingAddressCheckboxLabel'|trans([], 'checkout') }}</span>
        </label>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-sm-12">
      <div id="different-billing-address">
        <div class="row">
            <div class="row">
                <div class="col-sm-4">
                    {% include '@Example/partials/form/choose-title.html.twig' with {'containerClass':"title", 'selectId':"billing-title-select", 'selectName':"titleBilling", 'options':settings.titleBilling} %}
                </div>
            </div>
            <div class="row">
                {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':6, 'name':"firstNameBilling", 'labelKey':"form.firstName", 'required':true, 'value':addressForm.firstNameBilling, 'settings':settings.firstNameBilling} %}
                {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':6, 'name':"lastNameBilling", 'labelKey':"form.lastName", 'required':true, 'value':addressForm.lastNameBilling, 'settings':settings.lastNameBilling} %}
                {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':12, 'name':"streetNameBilling", 'labelKey':"form.addressOne", 'required':true, 'value':addressForm.streetNameBilling, 'settings':settings.streetNameBilling} %}
                {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':12, 'name':"additionalStreetInfoBilling", 'labelKey':"form.addressTwo", 'required':false, 'value':addressForm.additionalStreetInfoBilling, 'settings':settings.additionalStreetInfoBilling} %}
                {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':8, 'name':"cityBilling", 'labelKey':"form.city", 'required':true, 'value':addressForm.cityBilling, 'settings':settings.cityBilling} %}
                {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':4, 'name':"postalCodeBilling", 'labelKey':"form.postCode", 'required':true, 'value':addressForm.postalCodeBilling, 'settings':settings.postalCodeBilling} %}
                <div class="col-sm-6">
                    {% include '@Example/partials/form/choose-country.html.twig' with {'containerClass':"country", 'selectId':"billing-country-select", 'selectName':"countryBilling", 'options':settings.countriesBilling} %}
                </div>
                {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':6, 'name':"regionBilling", 'labelKey':"form.region", 'required':false, 'value':addressForm.regionBilling, 'settings':settings.regionBilling} %}
            </div>
            <hr>
            <div class="row">
                {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':6, 'name':"phoneBilling", 'labelKey':"form.phone", 'required':false, 'value':addressForm.phoneBilling, 'settings':settings.phoneBilling} %}
                {% include '@Example/partials/form/input-text.html.twig' with {'columnWidth':6, 'name':"emailBilling", 'labelKey':"form.email", 'required':true, 'value':addressForm.emailBilling, 'settings':settings.emailBilling} %}
            </div>
        </div>
      </div>
    </div>
  </div>
  {% include '@Example/partials/checkout/continue-checkout-button.html.twig' %}
</form>
