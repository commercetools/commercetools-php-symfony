<div class="my-account-wishlist-pagination hidden-xs">
  <div class="row">
    <div class="col-sm-4">
      <div class="my-account-wishlist-clear">
        <button>{{ content.clearWishlistBtn }}</button>
      </div>
    </div>
    <div class="col-sm-4 text-center custom-pagination">
      <ul class="page-numbers">
        {% include '@Example/partials/common/pagination.html.twig' with {'pagination':content.pagination} %}
      </ul>
    </div>
    <div class="col-sm-4">
      <div class="my-account-wishlist-per-page">
        <span class="wishlist-display-selector-title hidden-sm">{{ 'displaySelector.title'|trans([], 'catalog') }}</span>
        <select>
          {% for list_i in content.displaySelector.list %}
            <option value="{{ list_i.value }}" {% if list_i.selected %}selected{% endif %}>{{ list_i.label }}</option>
          {% endfor %}
        </select>
      </div>
    </div>
  </div>
</div>
