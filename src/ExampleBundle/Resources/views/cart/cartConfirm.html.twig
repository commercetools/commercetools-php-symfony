{% extends "@Example/layout.html.twig" %}

{% block container %}

    {% include '@Example/partials/address.html.twig' with {address: cart.getShippingAddress} %}

    {% if cart.getBillingAddress %}
        {% include '@Example/partials/address.html.twig' with {address: cart.getBillingAddress} %}
    {% else %}
        {% include '@Example/partials/address.html.twig' with {address: cart.getShippingAddress} %}
    {% endif %}

    {% include '@Example/partials/shippingProvider.html.twig' with {shippingInfo: cart.getShippingInfo} %}

    {% include '@Example/partials/cartView.html.twig' with {cart: cart} %}

{% endblock %}
